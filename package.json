{
  "name": "multi-engineer-claude-sdk",
  "version": "1.0.0",
  "description": "Claude Code SDK for multi-engineer project",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build:electron": "cd electron && tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "parallel-dev": "ts-node src/parallel-dev-electron.ts",
    "parallel-dev-cli": "ts-node src/parallel-dev.ts",
    "electron": "cd electron && electron .",
    "electron:build": "npm run build:electron && npm run electron",
    "parallel-dev-gui": "ts-node src/parallel-dev-electron.ts --electron"
  },
  "dependencies": {
    "@anthropic-ai/claude-code": "latest",
    "blessed": "^0.1.81",
    "blessed-contrib": "^4.11.0",
    "chalk": "^5.4.1",
    "split.js": "^1.6.5",
    "strip-ansi": "^7.1.0",
    "uuid": "^9.0.0",
    "xterm": "^5.3.0",
    "xterm-addon-fit": "^0.8.0",
    "xterm-addon-search": "^0.13.0",
    "xterm-addon-web-links": "^0.9.0"
  },
  "devDependencies": {
    "@types/blessed": "^0.1.25",
    "@types/node": "^20.0.0",
    "@types/uuid": "^9.0.0",
    "electron": "^28.3.3",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
